{
  "name": "react-redux-ssr-webpack2-example",
  "description": "Example of an isomorphic (universal) webapp using react redux, ssr and hot reloading",
  "author": "Faradey27 <andrii.tiertyshnyi@gmail.com> (http://github.com/faradey27)",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/faradey27/react-redux-ssr-webpack2-example"
  },
  "homepage": "https://github.com/faradey27/react-redux-ssr-webpack2-example",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack2",
    "ssr",
    "server side",
    "hot reloading",
    "redux",
    "starter",
    "boilerplate"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "node ./build-scripts/bin start",
    "start-prod": "node ./build-scripts/bin start:prod",
    "test": "node ./build-scripts/bin test",
    "test:unit": "node ./build-scripts/bin test:unit",
    "test:e2e": "node ./build-scripts/bin test:e2e",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "analysis": "cross-env ANALYSIS=1 npm run build",
    "analysis:dead-code": "cross-env DEAD_CODE=1 npm run build",
    "build": "node build-scripts/scripts/build.js && npm run postinstall",
    "postinstall": "webpack --colors --display-error-details --config build-scripts/vendor.config.js",
    "lint": "node ./build-scripts/bin lint",
    "locales:po": "node ./build-scripts/bin locales:po",
    "locales:json": "node ./build-scripts/bin locales:json",
    "webpack-defaults": "webpack-defaults"
  },
  "pre-commit": [
    "lint",
    "test:unit"
  ],
