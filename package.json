  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/ssr/services/bootstrap.js",
      "!src/ssr/middlewares/http2Push.js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleNameMapper": {
      "^.*[.](gif|ttf|eot|svg|png|jpg)$": "<rootDir>/test/fileMock.js"
    }
  },
  "engines": {
    "node": "8.1.0"
  }
